page {
  height: auto;
  min-height: 100%;
  background-color: #070707;
}
.wrapper {
  padding: 30rpx;
  align-items: flex-start;
}
.container {
  width: 100%;
}
.item {
  width: 100%;
}
.item .container-h {
  width: 100%;
  margin-top: 10rpx;
  justify-content: flex-start;
  align-items: flex-start;
}
.m-t {
  margin-top: 30rpx!important;
}
.m-t-lg {
  margin-top: 100rpx;
}
.main-btn {
  width: 500rpx;
}

text, label {
  color: #fff;
}

.radio {
  margin-right: 80rpx;
}

.center-container {
  width: 100%;
  justify-content: center;
  align-items: center;
}
.center-container image {
  width: 500rpx;
}
