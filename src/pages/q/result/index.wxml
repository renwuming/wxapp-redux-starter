<view class="container wrapper">
  <view class="container">
    <view wx:for="{{reslist}}" wx:key="item" wx:for-index="index" class="item m-t">
      <text>{{item.title}}</text>
      <view class="container-h player-list m-t">
        <image wx:if="{{player.player}}" wx:for="{{item.list}}" wx:for-item="player" wx:key="item" src="{{player.player.avatarUrl}}" bindtap="goto" data-answer="{{player.data}}" data-player="{{player.player}}"></image>
        <image wx:if="{{!player.player}}" wx:for="{{item.list}}" wx:for-item="player" wx:key="item" src="/img/head.jpg" bindtap="goto" data-answer="{{player.data}}" data-player="{{player.player}}"></image>
      </view>
    </view>
  </view>

</view>
