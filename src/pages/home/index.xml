<view class="container wrapper">
  <view class="top part container-h">
    <view class="container">
      <image src="{{userInfo.avatarUrl}}" mode="aspectFit" class="portrait"></image>
      <text class="nick">{{userInfo.nickName}}</text>
    </view>
  </view>
  <view class="part container-h">
    <view class="friend-part" bindtap="navigateTo" data-url="/pages/FriendTest/home/index">
      <image src="https://mir-s3-cdn-cf.behance.net/project_modules/fs/b61b1c60982081.5a5f848a67099.jpg" mode="aspectFill"></image>
      <text class="float-text DIN">友情鉴定</text>
    </view>
  </view>
  <view class="part container-h " style="margin-top:20rpx;">
    <view class="baozou-part" bindtap="navigateTo" data-url="/pages/Baozou/home/index">
      <image src="https://mir-s3-cdn-cf.behance.net/project_modules/fs/33671c60982081.5a5f848a64beb.jpg" mode="aspectFill"></image>
      <text class="float-text DIN">暴走测试</text>
    </view>
    <view class="personal-part" bindtap="navigateTo" data-url="/pages/Setting/personal/index">
      <image src="https://mir-s3-cdn-cf.behance.net/project_modules/1400/8c7c1e60982081.5a5f848a631b9.jpg" mode="aspectFill"></image>
      <text class="float-text DIN">个人中心</text>
    </view>
  </view>
</view>